[Unit]
Description=GNOME Kiosk Notification Daemon
PartOf=org.gnome.Kiosk.target
After=org.gnome.Kiosk.target

[Service]
Type=dbus
BusName=org.freedesktop.Notifications
ExecStart=@bindir@/gnome-service-client --tag=gnome-kiosk-notification @libexecdir@/gnome-kiosk-notification-daemon
Restart=on-failure
